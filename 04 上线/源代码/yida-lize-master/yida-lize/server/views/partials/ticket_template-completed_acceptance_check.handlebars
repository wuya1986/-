<div class="form-group form">
  <label class="col-sm-3 control-label">施工类别</label>
  <div class="col-sm-9">
    <select class="form-control" id="completed_acceptance_checks_type" name="form_data[completed_acceptance_checks_type]" required
    {{#if (or (intersection @root.session.user.acl_roles '["ticket_manager"]') (and  (eq data.progress '已受理') (intersection @root.session.user.acl_roles '["building_manager"]')))}}
    {{else}}
    disabled
    {{/if}}>
      <option value="租户装修">租户装修</option>
      <option value="项目委托施工">项目委托施工</option>
      <option value="外部单位项目内施工">外部单位项目内施工</option>
    </select>
  </div>
</div>
<div class="form-group form">
  <label class="col-sm-3 control-label">施工位置</label>
  <div class="col-sm-4">
    <div class="input-group">
      <input type="number" pattern="[0-9]*" class="form-control text-right" name="form_data[completed_acceptance_checks_floor]" value="{{data.form_data.completed_acceptance_checks_floor}}" required
      {{#if (or (intersection @root.session.user.acl_roles '["ticket_manager"]') (and  (eq data.progress '已受理') (intersection @root.session.user.acl_roles '["building_manager"]')))}}
      {{else}}
      disabled
      {{/if}}/>
      <span class="input-group-addon">层</span>
    </div>
  </div>
  <div class="col-sm-5">
    <div class="input-group">
      <input type="number" pattern="[0-9]*" class="form-control text-right" name="form_data[completed_acceptance_checks_number]" value="{{data.form_data.completed_acceptance_checks_number}}" required
      {{#if (or (intersection @root.session.user.acl_roles '["ticket_manager"]') (and  (eq data.progress '已受理') (intersection @root.session.user.acl_roles '["building_manager"]')))}}
      {{else}}
      disabled
      {{/if}}/>
      <span class="input-group-addon">号</span>
    </div>
  </div>
</div>
<div class="form-group form">
  <label class="col-sm-3 control-label"></label>
  <div class="col-sm-9">
    <div class="input-group">
      <input type="text" class="form-control text-right" name="form_data[completed_acceptance_checks_area]" value="{{data.form_data.completed_acceptance_checks_area}}" required
      {{#if (or (intersection @root.session.user.acl_roles '["ticket_manager"]') (and  (eq data.progress '已受理') (intersection @root.session.user.acl_roles '["building_manager"]')))}}
      {{else}}
      disabled
      {{/if}}/>
      <span class="input-group-addon">区域</span>
    </div>
  </div>
</div>
<div class="form-group form">
  <label class="col-sm-3 control-label">租户/安排施工单位名称</label>
  <div class="col-sm-9">
    <input type="text" class="form-control" name="form_data[completed_acceptance_checks_tenantry_unit_name]"  value="{{data.form_data.completed_acceptance_checks_tenantry_unit_name}}" required
    {{#if (or (intersection @root.session.user.acl_roles '["ticket_manager"]') (and  (eq data.progress '已受理') (intersection @root.session.user.acl_roles '["building_manager"]')))}}
    {{else}}
    disabled
    {{/if}}/>
  </div>
</div>
<div class="form-group form">
  <label class="col-sm-3 control-label">联系人</label>
  <div class="col-sm-9">
    <input type="text" class="form-control" name="form_data[completed_acceptance_checks_tenantry_contact]" value="{{data.form_data.completed_acceptance_checks_tenantry_contact}}" required
    {{#if (or (intersection @root.session.user.acl_roles '["ticket_manager"]') (and  (eq data.progress '已受理') (intersection @root.session.user.acl_roles '["building_manager"]')))}}
    {{else}}
