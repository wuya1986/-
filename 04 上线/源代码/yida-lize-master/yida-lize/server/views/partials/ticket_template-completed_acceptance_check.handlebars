<div class="form-group form">
  <label class="col-sm-3 control-label">初步验收结论</label>
  <div class="col-sm-9">
    <select class="form-control" id="completed_acceptance_checks_result" name="form_data[completed_acceptance_checks_result]" required
    {{#if (or (intersection @root.session.user.acl_roles '["ticket_manager"]') (and (or (eq data.progress '工程经理初检') (eq data.progress '安保经理初检')) (intersection @root.session.user.acl_roles '["repair_manager", "security_manager"]')))}}
    {{else}}
    disabled
    {{/if}}
    >
      <option value="未验收">未验收</option>
      <option value="验收合格">验收合格</option>
      <option value="待整改后复验">待整改后复验</option>
    </select>
  </div>
</div>
<div class="form-group form">
  <label class="col-sm-3 control-label">待整改项</label>
  <div class="col-sm-9">
    <textarea class="form-control" name="form_data[completed_acceptance_checks_rectification_items]" required
    {{#if (or (intersection @root.session.user.acl_roles '["ticket_manager"]') (and (or (eq data.progress '工程经理初检') (eq data.progress '安保经理初检')) (intersection @root.session.user.acl_roles '["repair_manager", "security_manager"]')))}}
    {{else}}
    disabled
    {{/if}}
    >{{data.form_data.completed_acceptance_checks_rectification_items}}</textarea>
  </div>
</div>
