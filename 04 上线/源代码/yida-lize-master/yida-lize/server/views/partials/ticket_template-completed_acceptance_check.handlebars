{{#extend 'scripts'}}
<script>
 $(function() {
   $("#completed_acceptance_checks_type").val('{{data.form_data.completed_acceptance_checks_type}}');
   $("#completed_acceptance_checks_result").val('{{data.form_data.completed_acceptance_checks_result}}');
